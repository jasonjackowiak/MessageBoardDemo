<!DOCTYPE html>
<html lang="en" data-ng-app="@ViewBag.InitModule">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <!-- Change title, description and keywords -->
    <title>@ViewBag.Title - Evil Jason's Bucks thingy</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="keywords" content="">

    <!-- HTML5 Support for IE -->
    <!--[if lt IE 9]>
    <script src="js/html5shim.js"></script>
    <![endif]-->
    <!-- Google Web Font -->
    <link href='http://fonts.googleapis.com/css?family=Dosis:200,400,600' rel='stylesheet' type='text/css'>

    <!-- Stylesheets -->
    <link href="~/Content/Template/style/bootstrap.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/Template/style/font-awesome.css">
    <!--[if IE 7]>
    <link rel="stylesheet" href="style/font-awesome-ie7.css">
    <![endif]-->


    <link href="~/Content/Template/style/ddlevelsmenu-base.css" rel="stylesheet">
    <link href="~/Content/Template/style/ddlevelsmenu-topbar.css" rel="stylesheet">
    <link href="~/Content/Template/style/style.css" rel="stylesheet">
    <!-- Change the template color below. Supported colors blue,red and green. -->
    <link href="~/Content/Template/style/blue.css" rel="stylesheet">
    <link href="~/Content/Template/style/bootstrap-responsive.css" rel="stylesheet">
    <link href="~/Content/Template/style/flexslider.css" rel="stylesheet">
    <link href="~/Content/Template/style/prettyPhoto.css" rel="stylesheet">
    @*Our Custom*@
    <link href="~/Content/Site.css" rel="stylesheet" />
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Content/Template/img/favicon/favicon.png">
</head>

<body>

    <!-- Header -->
    <div class="fixedbar">
        <div class="container">
            <div class="row">
                <div class="span12">
                    <div class="logo">
                        <div class="pull-left">
                            <!-- Logo with font awesome icon (icon-camera-retro). Replace it with your logo. Replace # with your site link -->
                            <h1><a href="/"><i class="icon-camera-retro"></i>Evil Jason's Bucks thingy</a></h1>
                        </div>
                        <div class="navi">
                            <!-- Navigation Starts Here -->
                            <div class="pull-left">
                                <div id="ddtopmenubar" class="mattblackmenu">
                                    <!-- Main navigation -->
                                    <ul>
                                        @*<li>@Html.ActionLink("Home", "Index", "Home")</li>
                                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                                        <li>@Html.ActionLink("About", "About", "Home")</li>*@
                                        @*<li>@Html.ActionLink("Leave a message", "MyMessages", "Home")</li>*@
                                        <li></li>
                                    </ul>
                                    
                                </div>

                            </div>
                            <!-- Navigation Ends Here -->
                        </div>
                    </div>
                    <div class="share">
                        <div class="pull-right">
                            <!-- Share it link for slider -->
                            <p>@Html.Partial("_LoginPartial")</p>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

    <!-- Main content -->

    <div class="main">
        <div class="container">
            <div class="row">
                @RenderBody()
            </div>
        </div>
    </div>

    <!-- Foot -->
    <!-- Three column footer -->

    <div class="foot">
        <div class="container">
            <div class="row">
                <div class="span4">
                    <div class="padd">
                        <!-- Add class "widget" -->
                        <div class="widget">
                            <!-- Title -->
                            <h3>Someone says...</h3>
                            <!-- Content -->
                            <!-- Below is the blockquote -->
                            <blockquote>
                                <p>Insert amusing or related quote for Jas here</p>
                                <!-- Author information -->
                                <small>Brad</small>
                            </blockquote>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Footer -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="span12">
                    <!-- Copyright information -->
                    <div class="fpadd">Copyright &copy; 2015 - <a href="#">Chow's Last Days of Freedom</a> / <a href="#">Disclaimer</a> / <a href="#">Privacy Policy</a></div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </footer>

    <!-- JS -->
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/easing.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script type="text/javascript" src="~/Scripts/ddlevelsmenu.js"></script>
    <script type="text/javascript" src="~/Scripts/flexslider.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.prettyPhoto.js"></script>
    <script type="text/javascript" src="~/Scripts/custom.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>